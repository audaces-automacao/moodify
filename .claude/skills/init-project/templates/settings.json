{
  "permissions": {
    "allow": [
      "Bash(npx ng update:*)",
      "Bash(npm run build:*)",
      "Bash(npm run lint)",
      "Bash(npm ls:*)",
      "Bash(npm test:*)",
      "Bash(npx ng test:*)",
      "Bash(npm run test:ci:*)"
    ],
    "deny": [
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(git rebase:*)",
      "Bash(git reset --hard:*)",
      "Bash(git push --force:*)"
    ]
  }
}
